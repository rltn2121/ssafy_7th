	static int dx[] = {-1,0,1,0};
	static int dy[] = {0,1,0,-1};
	public static void main(String[] args) throws Exception{
		input();
		
		q.add(shark);
		
		while(!q.isEmpty()) {
			int size = q.size();
			while(size-- > 0) {
				
